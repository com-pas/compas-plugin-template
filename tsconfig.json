    "compilerOptions": {
      "target": "ESNext",
      "module": "CommonJS",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "paths": {
        "@com-pas/compas-plugin": ["./packages/**/src"],
      }
    },
    "exclude": [
      "./**/test/**"
    ]
  }
  
  